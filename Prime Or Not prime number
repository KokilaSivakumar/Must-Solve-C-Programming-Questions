#include <stdio.h>
int main() 
{
    int n,i,factor = 0;
    printf("enter the value of n: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(n % i == 0)
        {
            factor = factor + 1;
        }
    }
    if(factor == 2)
    {
        printf("it is a Prime number");
    }
    else{
        printf("It is not a prime number");
    }
   return 0;
}
