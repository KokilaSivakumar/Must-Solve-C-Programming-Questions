#include <stdio.h>
int main() 
{
    int n,i=0;
    printf("Enter the Number: ");
    scanf("%d",&n);
    printf("The factors of the number are/is: ");
    for(i=1;i<=n;i++)
    {
        if(n % i == 0)
        {
            printf("\n %d",i);
        }
    }
   return 0;
}

//not a factor then if condition should be changed to n % i != 0.
//To count the number of factors declare count = 0 and count++ should be added next to line12 (print statement) 
